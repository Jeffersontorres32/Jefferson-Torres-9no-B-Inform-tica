Proceso Aprobo_Suspendio_Supletorio
	Definir N,A Como Caracter;
	Definir T1, T2, T3, PA Como Real;
	Escribir " Escriba un nobre y sus dos apellido" ;
	Leer N;
	Escribir " Escriba el nombre de la materia" ;
	Leer A;
	Escribir " Escriba su calificaci√≥n trimestral" ;
	Leer T1,T2,T3;
	PA<-(T1+T2+T3)/3;
	Si PA>=7 Entonces
		Escribir " El estudiante" , N, " Paso de la materia" , A, "Con una nota de " , Pa;
	SiNo
		Si PA<=7 y P>=4 Entonces
			Escribir " El estudiante" , N, " Se quedo supletorio en la materia" , A, "Con una nota de " , Pa;
		SiNo
			Escribir " El estudiante" , N, " Se quedo en la materia" , A, "Con una nota de " , Pa;
		FinSi
    FinSi
FinProceso
