Algoritmo Vectorial Aleatorio

Definir vector Como Entero;
Dimension vector[10];
Definir i Como Entero;

Para i = 1 Hasta 10 Hacer
	vector[i] = Aleatorio(1, 100);
FinPara;

Escribir "El vector de nÃºmeros aleatorios es:";
Para i = 1 Hasta 10 Hacer
	Escribir vector[i];
FinPara;

// Ordenar el vector de forma ascendente
Para i = 1 Hasta 9 Hacer
	Para j = i + 1 Hasta 10 Hacer
		Si vector[i] > vector[j] Entonces
			Definir temp Como Entero;
			temp = vector[i];
			vector[i] = vector[j];
			vector[j] = temp;
		FinSi;
	FinPara;
FinPara;

Escribir "El vector ordenado es:";
Para i = 1 Hasta 10 Hacer
	Escribir vector[i];
FinPara;
FinAlgoritmo
