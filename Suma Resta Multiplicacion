Proceso Suma_Resta_multiplicacion
	Definir N1,N2,N3 Como Entero;
	Definir S,R,M Como Real;
	Escribir 'Escribe el primer número';
	Leer N1;
	Escribir 'Escribe el segun número';
	Leer N2;
	Escribir 'Escribe el tercer número';
	Leer N3;
	S <- N1+N2+N3;
	R <- N1+N2+N3;
	M <- N1N2N3;
	Escribir 'El resultado de tu suma es ',S;
	Escribir 'El resultado de tu resta es ',R;
	Escribir 'El resultado de tu Multiplicación es ',M;
FinProceso
